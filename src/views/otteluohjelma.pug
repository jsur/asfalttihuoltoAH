include includes/header.pug
link(rel="stylesheet" href="/styles/otteluohjelma.css" type="text/css")
h1 Otteluohjelma
body(id='bootstrap-override')
	div(class='container-fluid')
		div(class='schedule')
			table(class='table table-striped table-responsive')
				thead
					tr
						th Päivä
						th Aika
						th Paikka
						th Koti
						th Vieras
						th Tulos
				tbody
					each value, i in data.games
						tr
							- var str = data.games[i].GameDate
							- var str2 = data.games[i].GameTime
							- var gameDate = str.substr(0,5);
							- var gameTime = str2.substr(0,5);
							td= gameDate 
							td= gameTime
							td #{data.games[i].RinkName}
							td #{data.games[i].HomeTeamName}
							td #{data.games[i].AwayTeamName}
							td #{data.games[i].Result}
	include includes/footer.pug
	script(src="js/main.js")